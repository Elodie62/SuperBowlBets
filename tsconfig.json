{
  "compilerOptions": {
    "target": "ES2017",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "nodenext",
    "module": "NodeNext"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
